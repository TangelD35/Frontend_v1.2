# Variables de entorno para el frontend
# Copia este archivo como .env y ajusta los valores según tu configuración

# URL del API backend
VITE_API_URL=http://localhost:8000

# URL del WebSocket
VITE_WS_URL=ws://localhost:8000

# Configuración de la aplicación
VITE_APP_NAME="Sistema de Análisis FEBADOM"
VITE_APP_VERSION="2.0"

# Configuración de desarrollo
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# Configuración de autenticación
VITE_JWT_EXPIRY=3600

# Configuración de uploads
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=.csv,.xlsx,.xls,.json

# Configuración de WebSocket
VITE_WS_ENABLED=false
VITE_WS_RECONNECT_ATTEMPTS=5
VITE_WS_RECONNECT_DELAY=1000

# Configuración de análisis predictivo
VITE_PREDICTION_CONFIDENCE_THRESHOLD=0.7
VITE_MAX_SIMULATION_ITERATIONS=10000

# Configuración de exportación
VITE_DEFAULT_EXPORT_FORMAT=pdf
VITE_EXPORT_QUALITY=high# C
onfiguración PWA
